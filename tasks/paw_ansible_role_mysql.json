      "type": "Optional[String]"
    },
    "mysql_innodb_log_file_size": {
      "description": "Set .._log_file_size to 25% of buffer pool size.",
      "type": "Optional[String]"
    },
    "mysql_innodb_log_buffer_size": {
      "description": "Ansible role variable (default: 8M)",
      "type": "Optional[String]"
    },
    "mysql_innodb_flush_log_at_trx_commit": {
      "description": "Ansible role variable (default: 1)",
      "type": "Optional[String]"
    },
    "mysql_innodb_lock_wait_timeout": {
      "description": "Ansible role variable (default: 50)",
      "type": "Optional[String]"
    },
    "mysql_mysqldump_max_allowed_packet": {
      "description": "mysqldump settings.",
      "type": "Optional[String]"
    },
    "mysql_config_include_dir": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_root_username": {
      "description": "Ansible role variable (default: root)",
      "type": "Optional[String]"
    },
    "mysql_root_password": {
      "description": "Ansible role variable (default: root)",
      "type": "Optional[String]"
    },
    "mysql_user_name": {
      "description": "Ansible role variable (default: root)",
      "type": "Optional[String]"
    },
    "mysql_user_password": {
      "description": "Ansible role variable (default: root)",
      "type": "Optional[String]"
    },
    "mysql_user_home": {
      "description": "or sudo access",
      "type": "Optional[String]"
    },
    "mysql_root_home": {
      "description": "The default root user installed by mysql - almost always root",
      "type": "Optional[String]"
    },
    "mysql_root_password_update": {
      "description": "Set this to `true` to forcibly update the root password.",
      "type": "Optional[Boolean]"
