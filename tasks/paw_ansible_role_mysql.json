      "type": "Optional[String]"
    },
    "mysql_query_cache_type": {
      "description": "their previous MySQL counterparts",
      "type": "Optional[String]"
    },
    "mysql_query_cache_size": {
      "description": "Ansible role variable (default: 16M)",
      "type": "Optional[String]"
    },
    "mysql_query_cache_limit": {
      "description": "Ansible role variable (default: 1M)",
      "type": "Optional[String]"
    },
    "mysql_max_connections": {
      "description": "Ansible role variable (default: 151)",
