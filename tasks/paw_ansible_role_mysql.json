    "mysql_copy_root_user_mycnf": {
      "description": "Whether to copy root user's config file with credentials to their homedir",
      "type": "Optional[Boolean]"
    },
    "mysql_enablerepo": {
      "description": "for RedHat systems (and derivatives).",
      "type": "Optional[String]"
    },
    "mysql_python_package_debian": {
      "description": "Ansible role variable (default: python3-mysqldb)",
      "type": "Optional[String]"
    },
    "mysql_skip_name_resolve": {
      "description": "",
      "type": "Optional[Boolean]"
    },
    "mysql_log_file_group": {
      "description": "Log file settings.",
      "type": "Optional[String]"
    },
    "mysql_slow_query_log_enabled": {
      "description": "Slow query log settings.",
      "type": "Optional[Boolean]"
    },
    "mysql_config_include_files": {
      "description": "Ansible role variable (default: [])",
      "type": "Optional[Array]"
    },
    "mysql_databases": {
      "description": "Databases.",
      "type": "Optional[Array]"
    },
    "mysql_users": {
      "description": "Users.",
      "type": "Optional[Array]"
    },
    "mysql_disable_log_bin": {
      "description": "",
      "type": "Optional[Boolean]"
    },
    "mysql_replication_role": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "mysql_replication_master": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
