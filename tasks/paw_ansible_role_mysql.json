    "mysql_port": {
      "description": "MySQL connection settings.",
      "type": "Optional[String]"
    },
    "mysql_socket": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_bind_address": {
      "description": "Ansible role variable (default: 0.0.0.0)",
      "type": "Optional[String]"
    },
    "mysql_datadir": {
      "description": "Ansible role variable (default: /var/lib/mysql)",
      "type": "Optional[String]"
    },
    "mysql_pid_file": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_sql_mode": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_syslog_tag": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_log": {
      "description": "Logging settings.",
      "type": "Optional[String]"
    },
    "mysql_log_error": {
      "description": "",
      "type": "Optional[String]"
    },
    "mysql_slow_query_log_file": {
      "description": "",
      "type": "Optional[String]"
    },
